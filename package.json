{
  "name": "@proladon/shelter-ui",
  "version": "0.1.21",
  "type": "module",
  "description": "shelter vue ui components",
  "author": "Proladon",
  "license": "MIT",
  "keywords": [
    "vue",
    "components",
    "ui"
  ],
  "main": "dist/shelter-ui.cjs.js",
  "module": "dist/shelter-ui.js.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/shelter-ui.js.js",
      "require": "./dist/shelter-ui.cjs.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core.js.js",
      "require": "./dist/core.cjs.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lib:build": "vue-tsc -b && vite build",
    "lib:publish": "npm publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@tabler/icons-vue": "^3.34.1",
    "@unocss/reset": "^66.4.1",
    "@vueuse/core": "^13.6.0",
    "color": "^5.0.0",
    "reka-ui": "^2.4.1",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/color": "^4.2.0",
    "@types/node": "^22.14.1",
    "@unocss/transformer-directives": "^66.4.1",
    "@unocss/transformer-variant-group": "^66.4.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.90.0",
    "typescript": "^5.9.2",
    "unocss": "^66.4.1",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.0.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}