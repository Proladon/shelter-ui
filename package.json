{
  "name": "@proladon/shelter-ui",
  "version": "0.1.13",
  "type": "module",
  "description": "shelter vue ui components",
  "author": "Proladon",
  "license": "MIT",
  "keywords": [
    "vue",
    "components",
    "ui"
  ],
  "main": "dist/shelter-ui.cjs.js",
  "module": "dist/shelter-ui.js.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/shelter-ui.js.js",
      "require": "./dist/shelter-ui.cjs.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core.js.js",
      "require": "./dist/core.cjs.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lib:build": "vue-tsc -b && vite build",
    "lib:publish": "npm publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@tabler/icons-vue": "^3.31.0",
    "@unocss/reset": "^66.1.0-beta.12",
    "color": "^5.0.0",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.4.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/color": "^4.2.0",
    "@types/node": "^22.14.1",
    "@unocss/transformer-directives": "^66.1.0-beta.12",
    "@unocss/transformer-variant-group": "^66.1.0-beta.12",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.87.0",
    "typescript": "~5.7.2",
    "unocss": "^66.1.0-beta.12",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3",
    "vitepress": "^1.6.3",
    "vue-tsc": "^2.2.8"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
